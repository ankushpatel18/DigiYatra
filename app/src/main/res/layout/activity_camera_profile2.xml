<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.Camera_profile2">


    <ImageButton
        android:id="@+id/backBtn1"
        android:layout_width="@dimen/_19sdp"
        android:layout_height="@dimen/_25sdp"
        android:layout_marginStart="@dimen/_12sdp"
        android:layout_marginTop="@dimen/_8sdp"
        android:background="#EBF3FC"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.003"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_arrow_back"
        app:tint="#002E56" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_8sdp"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginEnd="@dimen/_125sdp"
        android:text="Add Credential"
        android:textColor="#002E56"
        android:textSize="@dimen/_18ssp"
        android:textStyle="bold"
        android:fontFamily="@font/poppins_bold"
        app:layout_constraintBottom_toBottomOf="@+id/backBtn1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.002"
        app:layout_constraintStart_toEndOf="@+id/backBtn1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.627" />

    <ImageView
        android:id="@+id/imageDot2"
        android:layout_width="@dimen/_10sdp"
        android:layout_height="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_140sdp"
        android:layout_marginTop="@dimen/_25sdp"
        android:background="#EBF3FC"
        app:tint="#01893E"
        app:layout_constraintEnd_toStartOf="@+id/imageDot3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:srcCompat="@drawable/ic_record" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Review"
        android:textColor="#333333"
        android:textSize="@dimen/_8ssp"
        android:fontFamily="@font/roboto_regular"
        android:drawableTint="#01893E"
        app:layout_constraintEnd_toEndOf="@+id/imageDot3"
        app:layout_constraintStart_toStartOf="@+id/imageDot3"
        app:layout_constraintTop_toBottomOf="@+id/imageDot3" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="@dimen/_10sdp"
        android:layout_height="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_45sdp"
        android:layout_marginTop="@dimen/_25sdp"
        android:background="#EBF3FC"
        app:layout_constraintEnd_toStartOf="@+id/imageDot2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:srcCompat="@drawable/ic_record"
        app:tint="#01893E" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Setup Face ID"
        android:textColor="#333333"
        android:fontFamily="@font/roboto_regular"

        android:textSize="@dimen/_8ssp"
        app:layout_constraintTop_toBottomOf="@id/imageDot2"
        app:layout_constraintStart_toStartOf="@id/imageDot2"
        app:layout_constraintEnd_toEndOf="@id/imageDot2"
         />

    <ImageView
        android:id="@+id/imageDot3"
        android:layout_width="@dimen/_10sdp"
        android:layout_height="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_240sdp"
        android:layout_marginTop="@dimen/_25sdp"
        android:layout_marginEnd="@dimen/_45sdp"
        android:background="#EBF3FC"
        app:tint="#01893E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.848"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:srcCompat="@drawable/ic_record" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Verify ID"
        android:textColor="#333333"
        android:textSize="@dimen/_8ssp"
        android:fontFamily="@font/roboto_regular"
        app:layout_constraintEnd_toEndOf="@id/imageView16"
        app:layout_constraintStart_toStartOf="@id/imageView16"
        app:layout_constraintTop_toBottomOf="@+id/imageView16" />




    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_40sdp"
        android:fontFamily="@font/poppins"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView31"
        android:paddingBottom="@dimen/_20sdp"
        android:background="@drawable/textbox_border"
        android:backgroundTint="#EBF3FC">
        <ImageView
            android:id="@+id/img_aadhar"
            android:layout_width="@dimen/_100sdp"
            android:layout_height="@dimen/_100sdp"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="31dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/profile" />
        <ImageView
            android:id="@+id/imageView35"
            android:layout_width="@dimen/_29sdp"
            android:layout_height="@dimen/_29sdp"
            app:layout_constraintBottom_toBottomOf="@id/img_aadhar"
            app:layout_constraintEnd_toEndOf="@id/img_aadhar"
            app:srcCompat="@drawable/vector"
            android:background="@drawable/bg_circle"
            android:backgroundTint="#01893E"/>

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_55sdp"
            android:layout_marginTop="@dimen/_8sdp"
            android:layout_marginEnd="@dimen/_56sdp"
            android:layout_marginBottom="@dimen/_85sdp"
            android:text="Review Your Details"
            android:fontFamily="@font/poppins"
            android:textColor="#000000"
            android:textStyle="bold"
            android:textSize="@dimen/_15ssp"
            app:layout_constraintBottom_toBottomOf="@+id/constraint"
            app:layout_constraintEnd_toEndOf="@+id/constraint"
            app:layout_constraintHorizontal_bias="0.666"
            app:layout_constraintStart_toStartOf="@+id/constraint"
            app:layout_constraintTop_toBottomOf="@id/img_aadhar"
            app:layout_constraintVertical_bias="0.062" />
        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_15sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_10sdp"
            android:text="Name"
            android:fontFamily="@font/poppins"
            android:textColor="#666666"
            android:textSize="@dimen/_12ssp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2" />
        <TextView
            android:id="@+id/txt_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15sdp"
            android:text="Chandu"
            android:textColor="#666666"
            android:textSize="@dimen/_12ssp"
            android:fontFamily="@font/poppins"
            app:layout_constraintTop_toTopOf="@id/textView19"
            app:layout_constraintBottom_toBottomOf="@id/textView19"
            app:layout_constraintEnd_toEndOf="parent" />
        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AADHAAR ID"
            android:textColor="#666666"
            android:textSize="@dimen/_12ssp"
            android:layout_marginTop="@dimen/_15sdp"
            android:fontFamily="@font/poppins"
            app:layout_constraintStart_toStartOf="@id/textView19"
            app:layout_constraintTop_toBottomOf="@id/textView19" />
        <TextView
            android:id="@+id/txt_aadhar_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="XXXXXXXXX"
            android:textColor="#666666"
            android:textSize="@dimen/_12ssp"
            android:fontFamily="@font/poppins"
            app:layout_constraintStart_toStartOf="@id/txt_name"
            app:layout_constraintTop_toTopOf="@id/textView21"
            app:layout_constraintBottom_toBottomOf="@id/textView21"
             />
        <TextView
            android:id="@+id/txt_phone1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mobile No"
            android:textColor="#666666"
            android:textSize="@dimen/_12ssp"
            android:layout_marginTop="@dimen/_15sdp"
            android:fontFamily="@font/poppins"
            app:layout_constraintStart_toStartOf="@id/textView19"
            app:layout_constraintTop_toBottomOf="@id/textView21"
            />
        <TextView
            android:id="@+id/txt_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="XXXXXXXXX"
            android:textColor="#666666"
            android:textSize="@dimen/_12ssp"
            android:fontFamily="@font/poppins"
            app:layout_constraintStart_toStartOf="@id/txt_name"
            app:layout_constraintTop_toTopOf="@id/txt_phone1"
            app:layout_constraintBottom_toBottomOf="@id/txt_phone1" />
        <com.google.android.material.button.MaterialButton
            android:id="@+id/btn_accept"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Accept"
            android:layout_marginTop="@dimen/_20sdp"
            android:backgroundTint="#002E56"
            android:textColor="@color/white"
            android:textAllCaps="false"
            android:fontFamily="@font/poppins_medium"
            style="@style/Widget.MaterialComponents.Button.OutlinedButton"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/btn_cancel"
            app:layout_constraintTop_toBottomOf="@id/txt_phone1"
            />
        <com.google.android.material.button.MaterialButton
            android:id="@+id/btn_cancel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/cancel"
            android:fontFamily="@font/poppins_medium"
            android:layout_marginTop="@dimen/_20sdp"
            android:textAllCaps="false"
            style="@style/Widget.MaterialComponents.Button.OutlinedButton"
            android:backgroundTint="@color/white"
            android:textColor="#002E56"
            app:strokeColor="#002E56"
            android:textSize="@dimen/_10ssp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/btn_accept"
            app:layout_constraintTop_toBottomOf="@id/txt_phone1"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>